{
  "name": "@owl-systems/ui-kit",
  "version": "0.1.0",
  "description": "UI Kit for INFLUENCER HUB based in Owl Systems",
  "type": "module",
  "repository": "https://github.com/DanielBGomez/sellia-take-home-challenge",
  "author": "Cesar Daniel Bañuelos Gómez <contact@danielbgomez.com>",
  "license": "MIT",
  "private": true,
  "files": [
    "dist"
  ],
  "directories": {
    "dist": "dist"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./mui-material": {
      "import": "./dist/mui-material.esm.js",
      "require": "./dist/mui-material.cjs.js",
      "types": "./dist/mui-material.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:production": "vite build --mode production",
    "build:types": "tsc --emitDeclarationOnly",
    "build:storybook": "storybook build",
    "eslint": "eslint src --ext js,ts,tsx",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "storybook": "storybook dev -p 6006"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/outfit": "^5.2.5",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "6.4.8",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-interactions": "^8.6.4",
    "@storybook/cli": "^8.6.4",
    "@storybook/react-vite": "8.6.4",
    "@tailwindcss/vite": "^4.0.15",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitejs/plugin-react": "^4.3.4",
    "classnames": "^2.5.1",
    "eslint": "^9.21.0",
    "eslint-config-ts-react-important-stuff": "^3.0.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "prettier": "^3.5.3",
    "storybook": "^8.6.8",
    "tailwindcss": "^4.0.15",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.0.8"
  }
}
